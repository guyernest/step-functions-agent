{
  "agent_id": "HFLRYXFCIK",
  "agent_arn": "arn:aws:bedrock:us-west-2:672915487120:agent/HFLRYXFCIK",
  "alias_id": "PJT0AMPYXV",
  "agent_name": "web-search-agent",
  "region": "us-west-2",
  "invoke_url": "https://bedrock-agent-runtime.us-west-2.amazonaws.com",
  "state_machine_config": {
    "Comment": "Wrapper state machine for Agent Core agent: web-search-agent",
    "StartAt": "InvokeAgent",
    "States": {
      "InvokeAgent": {
        "Type": "Task",
        "Resource": "arn:aws:states:::bedrock:invokeAgent",
        "Parameters": {
          "AgentId": "HFLRYXFCIK",
          "AgentAliasId": "PJT0AMPYXV",
          "SessionId.$": "$.session_id",
          "InputText.$": "$.input_text",
          "EnableTrace": true,
          "EndSession": false
        },
        "ResultPath": "$.agent_response",
        "Next": "FormatResponse"
      },
      "FormatResponse": {
        "Type": "Pass",
        "Parameters": {
          "agent_messages": [
            {
              "role": "assistant",
              "content.$": "$.agent_response.completion"
            }
          ],
          "trace.$": "$.agent_response.trace",
          "session_id.$": "$.session_id",
          "citations.$": "$.agent_response.citations"
        },
        "End": true
      }
    }
  }
}