{
    "compilerOptions": {
      "target": "es2022",
      "strict": true,
      "preserveConstEnums": true,
      "noEmit": true,
      "sourceMap": false,
      "module":"commonjs",
      "moduleResolution":"node",
      "esModuleInterop": true, 
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true, 
      "isolatedModules": true, 
      "outDir": "./dist",
    },
    "exclude": ["node_modules", "**/*.test.ts", "dist"]
  }